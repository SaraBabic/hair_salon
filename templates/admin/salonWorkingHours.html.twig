{% block field %}
    {% if object.salonWorkingHours is not empty %}
        <table class="table">
            {% for s in object.salonWorkingHours %}
                <tr>
                    <th>{{ s.dayName }}</th>
                    <td>{{ s.totalWorkingHours }}</td>
                </tr>
            {% endfor %}
        </table>
    {% else %}
        <p style="padding-left: 25px;">No data.</p>
    {% endif %}
{% endblock %}